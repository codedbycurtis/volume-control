{
    "manifest_version": 3,
    "name": "Volume Control",
    "description": "Control the volume of individual tabs.",
    "version": "1.0.0",
    "action": {
      "default_popup": "html/volume-control.html",
      "default_icon": "images/equalizer.png"
    },
    "permissions": ["tabs", "scripting"],
    "host_permissions": ["<all_urls>"]
}